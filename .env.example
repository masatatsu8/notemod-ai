# Gemini API Key
GEMINI_API_KEY=your_api_key_here

# Authentication Settings
# Set to "true" or "1" to enable authentication, anything else disables it
# Note: Vite requires VITE_ prefix for client-side environment variables
VITE_AUTHENTICATION=false

# Login Credentials (only used if AUTHENTICATION is enabled)
VITE_APP_USERNAME=admin
VITE_APP_PASSWORD=password123
